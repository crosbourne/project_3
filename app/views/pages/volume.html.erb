<%= link_to image_tag("back.png", height: '30', width: '30', class: "back"), browse_path %> 
<a href="#footer"><h1>VOLUME</h1></a>



      <p>

        <% @volumes.to_a.each do |volume| %>
        <%= volume.name %>
        <%#= volume.description.try(:html_safe) %><br>
        <% if volume.image.present? %>
        <%= image_tag(volume.image["small_url"], height: '160', width: '110') %><br>
        <% else %>
        <%= image_tag("no_comic_cover.jpg", height: '160', width: '110')  %><br>
        <% end %>
        <% end %>
        </p>
